    "@xterm/xterm": "^6.0.0",
    "next": "16.1.6",
    "next-auth": "^5.0.0-beta.29",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "@corphish/eslint": "workspace:*",
    "@corphish/prettier": "workspace:*",
    "@corphish/tsconfig": "workspace:*",
    "@tailwindcss/postcss": "^4.1.18",
    "@types/node": "^24.9.0",
    "@types/react": "^19.2.2",
