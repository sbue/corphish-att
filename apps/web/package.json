{
  "name": "@corphish/web",
  "private": true,
  "type": "module",
  "scripts": {
    "clean": "rm -rf .turbo .next node_modules dist",
    "dev": "next dev",
    "build": "next build",
    "start": "node .next/standalone/apps/web/server.js",
    "lint": "eslint . --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,md}\"",
